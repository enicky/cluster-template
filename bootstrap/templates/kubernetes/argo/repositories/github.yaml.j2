---
apiVersion: v1
kind: Secret
metadata:
  name: github
  namespace: argo-system
  annotations:
    argocd.argoproj.io/sync-wave: '1'
  labels:
    argocd.argoproj.io/secret-type: repository
stringData:
  type: git
  url: #{ bootstrap_github_address }#
  #% if bootstrap_github_private_key %#
  sshPrivateKey: {{ .Values.sshPrivateKey }}
  #% endif %#

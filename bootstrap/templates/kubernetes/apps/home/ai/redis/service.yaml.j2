apiVersion: v1
kind: Service
metadata:
  name: #{ bootstrap_ai.redis.fullname | default('redis') }#
  namespace: #{ bootstrap_ai.namespace | default('ai') }#
  labels:
    app: #{ bootstrap_ai.redis.fullname | default('redis') }#
spec:
  type: ClusterIP
  ports:
    - port: 6379
      targetPort: 6379
      protocol: TCP
      name: redis
  selector:
    app: #{ bootstrap_ai.redis.fullname | default('redis') }#
